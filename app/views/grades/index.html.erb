<div class="container">

  <h2>Grades</h2>
  <div class="well">
    <%= link_to "New grade", new_grade_path, remote: true, class: "btn btn-primary" %>
  </div>
  <div class="new-grade"></div>

  <table id class="table table-bordered table-striped">
    <thead>
    <tr>
      <th>ID#</th>
      <th>Value</th>
      <th>Updated at</th>
      <th>Created at</th>
      <th>Actions</th>
    </tr>
    </thead>
    <tbody id="grades">
    <%= render @grades %>
    </tbody>
  </table>
  
</div>
